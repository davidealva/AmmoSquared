<div class="container">
  <div class="row">
    <header>
      <h1 class="text-center">Sign Up</h1>
      <div class="timeline text-center">
        <hr>
        <div class="step-image step-active">
          <h4>Step 1</h4>
        </div>
        <div class="step-image">
          <h4>Step 2</h4>
        </div>
        <div class="step-image">
          <h4>Step 3</h4>
        </div>
      </div>
    </header>
  </div>
  <div class="row">
    <h3 class="text-center">Create your log in and get started.</h3>
    <div id="newform"class="col-md-6 col-md-offset-3 ">
      <div class="well">
          <div class="center-block">
            <%= render "form" %>
          </div>
      </div>
    </div>
  </div>
</div>
